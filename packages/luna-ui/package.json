{
  "name": "luna-ui",
  "version": "0.1.0",
  "description": "A Vue3 UI component library with theme support",
  "type": "module",
  "main": "./dist/luna-ui.umd.cjs",
  "module": "./dist/luna-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/luna-ui.js",
      "require": "./dist/luna-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "vue": "^3.4.21",
    "typescript": "^5.4.0",
    "vite": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "vue-tsc": "^2.0.0"
  }
}


